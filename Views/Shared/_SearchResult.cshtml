@model List<string>
<ul class="list-group border border-3">
    @if(Model.Count > 0)
    {
        @foreach (var item in Model)
        {
            <li class=" list-group-item btn btn-outline-light text-start">@item</li>
        }

    }
    else
    {
        <li class="list-group-item">Không có kết quả</li>
    }
</ul>